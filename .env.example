# Example environment file - Copy this to .env and fill in your actual API keys
# NEVER commit your actual .env file to git!

# NEVER commit this file to git!
# Add .env to your .gitignore

# Search APIs
SERPAPI_API_KEY=your-key-here
TAVILY_API_KEY=your-key-here
BRAVE_API_KEY=your-key-here

# LangSmith
LANGSMITH_API_KEY=your-key-here

# Supabase
SUPABASE_URL=your-key-here
SUPABASE_KEY=your-key-here
SUPABASE_SERVICE_KEY=your-key-here
SUPABASE_DB_PASSWORD=your-key-here

# LLM APIs
GROQ_API_KEY=your-key-here
GOOGLE_API_KEY=your-key-here
ANTHROPIC_API_KEY=your-key-here
OPENAI_API_KEY=your-key-here

# Redis Configuration (for real-time collaboration)
REDIS_URL=your-key-here
REDIS_PASSWORD=your-key-here
REDIS_SSL=your-key-here

# Collaboration Settings
COLLAB_SESSION_TTL=your-key-here
COLLAB_MAX_PARTICIPANTS=your-key-here
COLLAB_ENABLE_RECORDING=your-key-here

# Workflow Settings
WORKFLOW_MAX_PARALLEL_STEPS=your-key-here
WORKFLOW_DEFAULT_TIMEOUT=your-key-here
WORKFLOW_RETRY_ATTEMPTS=your-key-here
WORKFLOW_TEMPLATE_DIR=your-key-here

# WebSocket Settings
WS_HEARTBEAT_INTERVAL=your-key-here
WS_MAX_CONNECTIONS_PER_USER=your-key-here
WS_MESSAGE_QUEUE_SIZE=your-key-here
